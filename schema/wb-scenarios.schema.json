{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "title": "Automation scenarios",
  "configFile": {
    "path": "/etc/wb-scenarios.conf",
    "service": "wb-rules"
  },
  "options": {
    "wb": {
      "disable_title": true
    }
  },
  "definitions": {
    "scenario": {
      "title": "Scenario",
      "headerTemplate": "{{ self.name }}",
      "oneOf": [
        {
          "$ref": "#/definitions/devicesControl"
        },
        {
          "$ref": "#/definitions/lightControl"
        }
      ],
      "_format": "wb-multiple",
      "options": {
        "keep_oneof_values": false,
        "disable_collapse": true,
        "disable_edit_json": true,
        "disable_properties": false,
        "wb": {
          "disable_panel": true
        }
      }
    },
    "devicesControl": {
      "title": "Devices control",
      "type": "object",
      "description": "devicesControl_description",
      "_format": "grid",
      "properties": {
        "scenarioType": {
          "type": "string",
          "enum": [
            "devicesControl"
          ],
          "default": "devicesControl",
          "options": {
            "hidden": true
          }
        },
        "enable": {
          "title": "generalScenarioGenerateRuleTitle",
          "type": "boolean",
          "default": true,
          "_format": "checkbox",
          "propertyOrder": 1,
          "options": {
            "grid_columns": 12
          }
        },
        "name": {
          "title": "generalScenarioName",
          "type": "string",
          "default": "Управление устройствами",
          "minLength": 1,
          "maxLength": 120,
          "propertyOrder": 2,
          "options": {
            "grid_columns": 12
          }
        },
        "id_prefix": {
          "title": "id_prefix_title",
          "type": "string",
          "description": "id_prefix_description",
          "_pattern_comment": "Запрещает пробелы, /, +, и #, а также ограничивает строку использованием только цифр, нижнего подчеркивания и английских букв",
          "pattern": "^[0-9a-zA-Z_]+$",
          "default": "devices_control",
          "minLength": 1,
          "maxLength": 120,
          "propertyOrder": 3,
          "options": {
            "grid_columns": 12,
            "patternmessage": "generalErrorRegexpPatternMessage"
          }
        },
        "inControls": {
          "title": "inControls_arr_title",
          "type": "array",
          "minItems": 1,
          "propertyOrder": 4,
          "_title_note": "When options-> compact: true - this title not shown",
          "description": "inControls_arr_description",
          "_format": "table",
          "options": {
            "disable_collapse": true,
            "grid_columns": 12,
            "array_controls_top": true,
            "disable_array_delete_last_row": true,
            "disable_array_reorder": true,
            "wb": {
              "disable_panel": true
            }
          },
          "items": {
            "title": "inControls_obj_title",
            "type": "object",
            "properties": {
              "control": {
                "type": "string",
                "propertyOrder": 1,
                "title": "generalMqttTopicNameTitle",
                "description": "generalMqttTopicNameDescription",
                "pattern": "^[^/+#]+/[^/+#]+$",
                "_format": "wb-autocomplete",
                "options": {
                "patternmessage": "generalErrorRegexpPatternMessage",
                  "wb": {
                    "data": "devices"
                  }
                },
                "_minLength_note": "Device name (1) + slash (1) + topic name (1)",
                "minLength": 3
              },
              "behaviorType": {
                "type": "string",
                "propertyOrder": 2,
                "title": "generalBehaviorTypeTitle",
                "enum": [
                  "whenChange",
                  "whenDisabled",
                  "whenEnabled"
                ],
                "default": "whenChange",
                "options": {
                  "enum_titles": [
                    "When Change",
                    "When Disabled (switch)",
                    "When Enabled (switch)"
                  ]
                }
              }
            },
            "required": [
              "control",
              "behaviorType"
            ]
          }
        },
        "outControls": {
          "type": "array",
          "minItems": 1,
          "propertyOrder": 5,
          "_title_note": "When options-> compact: true - this title not shown",
          "title": "outControls_arr_title",
          "description": "outControls_arr_description",
          "_format": "table",
          "options": {
            "disable_collapse": true,
            "grid_columns": 12,
            "array_controls_top": true,
            "disable_array_delete_last_row": true,
            "disable_array_reorder": true,
            "wb": {
              "disable_panel": true
            }
          },
          "items": {
            "title": "outControls_obj_title",
            "type": "object",
            "properties": {
              "control": {
                "type": "string",
                "propertyOrder": 1,
                "title": "generalMqttTopicNameTitle",
                "description": "generalMqttTopicNameDescription",
                "pattern": "^[^/+#]+/[^/+#]+$",
                "_format": "wb-autocomplete",
                "options": {
                  "patternmessage": "generalErrorRegexpPatternMessage",
                  "wb": {
                    "data": "devices"
                  }
                },
                "_minLength_note": "Device name (1) + slash (1) + topic name (1)",
                "minLength": 3
              },
              "behaviorType": {
                "type": "string",
                "propertyOrder": 2,
                "title": "outControls_behaviorType_title",
                "enum": [
                  "toggle",
                  "setEnable",
                  "setDisable",
                  "setValue",
                  "increaseValueBy",
                  "decreaseValueBy"
                ],
                "default": "setDisable",
                "options": {
                  "enum_titles": [
                    "Toggle (switch)",
                    "Enable (switch)",
                    "Disable (switch)",
                    "generalEnumSetValue",
                    "Increase Value By",
                    "Decrease Value By"
                  ]
                }
              },
              "actionValue": {
                "type": "number",
                "propertyOrder": 3,
                "title": "generalActionValueTitle",
                "description": "outControls_actionValue_description",
                "default": 0
              }
            },
            "required": [
              "control",
              "behaviorType"
            ]
          }
        }
      },
      "required": [
        "scenarioType",
        "enable",
        "name",
        "id_prefix",
        "inControls",
        "outControls"
      ]
    },
    "lightControl": {
      "title": "lightControlScenarioName",
      "type": "object",
      "description": "lightControlScenarioDescription",
      "_format": "grid",
      "options": {
        "display_required_only": true
      },
      "properties": {
        "componentVersion": {
          "type": "integer",
          "title": "Config version",
          "minimum": 1,
          "maximum": 256,
          "default": 1,
          "options": {
            "hidden": true
          }
        },
        "scenarioType": {
          "type": "string",
          "enum": [
            "lightControl"
          ],
          "default": "lightControl",
          "options": {
            "hidden": true
          }
        },
        "enable": {
          "title": "generalScenarioGenerateRuleTitle",
          "type": "boolean",
          "default": true,
          "_format": "checkbox",
          "propertyOrder": 1,
          "options": {
            "grid_columns": 12
          }
        },
        "isDebugEnabled": {
          "title": "generalScenarioIsDebugEnabled",
          "type": "boolean",
          "default": false,
          "_format": "checkbox",
          "propertyOrder": 2,
          "options": {
            "grid_columns": 12
          }
        },
        "name": {
          "title": "generalScenarioName",
          "type": "string",
          "default": "Управление светом",
          "minLength": 1,
          "maxLength": 120,
          "propertyOrder": 3,
          "options": {
            "grid_columns": 12
          }
        },
        "id_prefix": {
          "title": "id_prefix_title",
          "type": "string",
          "description": "id_prefix_description",
          "_pattern_comment": "Запрещает пробелы, /, +, и #, а также ограничивает строку использованием только цифр, нижнего подчеркивания и английских букв",
          "pattern": "^[0-9a-zA-Z_]+$",
          "default": "lightControl",
          "minLength": 1,
          "maxLength": 120,
          "propertyOrder": 4,
          "options": {
            "grid_columns": 12,
            "patternmessage": "generalErrorRegexpPatternMessage"
          }
        },
        "lightDevices": {
          "type": "object",
          "title": "lightControlLightDevicesTitle",
          "propertyOrder": 5,
          "options": {
            "disable_collapse": true,
            "disable_edit_json": true,
            "disable_properties": true
          },
          "properties": {
            "sensorObjects": {
              "type": "array",
              "title": "generalSensorArrTitle",
              "_title_note": "When options-> compact: true - this title not shown",
              "description": "lightControlLightDevicesArrDescription",
              "minItems": 1,
              "propertyOrder": 1,
              "_format": "table",
              "options": {
                "disable_collapse": true,
                "grid_columns": 12,
                "array_controls_top": true,
                "disable_array_delete_last_row": true,
                "disable_array_reorder": true,
                "wb": {
                  "disable_panel": true
                }
              },
              "items": {
                "title": "lightControlLightDevicesObjTitle",
                "type": "object",
                "properties": {
                  "mqttTopicName": {
                    "type": "string",
                    "propertyOrder": 1,
                    "title": "generalMqttTopicNameTitle",
                    "description": "generalMqttTopicNameDescription",
                    "pattern": "^[^/+#]+/[^/+#]+$",
                    "_format": "wb-autocomplete",
                    "options": {
                      "patternmessage": "generalErrorRegexpPatternMessage",
                      "wb": {
                        "data": "devices"
                      }
                    },
                    "_minLength_note": "Device name (1) + slash (1) + topic name (1)",
                    "minLength": 3
                  },
                  "behaviorType": {
                    "type": "string",
                    "propertyOrder": 2,
                    "title": "outControls_behaviorType_title",
                    "enum": [
                      "setEnable",
                      "setValueNumericInput"
                    ],
                    "default": "setEnable",
                    "options": {
                      "enum_titles": [
                        "Enable (switch)",
                        "generalEnumSetValue"
                      ]
                    }
                  },
                  "actionValue": {
                    "type": "number",
                    "propertyOrder": 3,
                    "title": "generalActionValueTitle",
                    "description": "outControls_actionValue_description",
                    "default": 0,
                    "options": {
                      "dependencies": {
                        "behaviorType": ["setValueNumericInput"]
                      }
                    }
                  }
                },
                "required": [
                  "mqttTopicName",
                  "behaviorType"
                ]
              }
            }
          }
        },
        "lightSwitches": {
          "type": "object",
          "title": "lightControlLightSwitchesTitle",
          "propertyOrder": 6,
          "options": {
            "disable_collapse": true,
            "disable_edit_json": true,
            "disable_properties": true
          },
          "properties": {
            "isDelayEnabled": {
              "title": "lightControlLightSwitchesEnableDelayTitle",
              "type": "boolean",
              "default": false,
              "_format": "checkbox",
              "propertyOrder": 1,
              "options": {
                "grid_columns": 12
              }
            },
            "delayToLightOffAndEnable": {
              "type": "number",
              "title": "lightControlLightSwitchesDelayTitle",
              "default": 3600,
              "propertyOrder": 2,
              "options": {
                "dependencies": {
                  "isDelayEnabled": true
                },
                "grid_columns": 12
              }
            },
            "sensorObjects": {
              "type": "array",
              "title": "generalSensorArrTitle",
              "_title_note": "When options-> compact: true - this title not shown",
              "description": "lightControlLightSwitchesArrDescription",
              "propertyOrder": 3,
              "_format": "table",
              "options": {
                "disable_collapse": true,
                "grid_columns": 12,
                "array_controls_top": true,
                "disable_array_delete_last_row": true,
                "disable_array_reorder": true,
                "wb": {
                  "disable_panel": true
                }
              },
              "items": {
                "title": "lightControlLightSwitchesObjTitle",
                "type": "object",
                "properties": {
                  "mqttTopicName": {
                    "type": "string",
                    "propertyOrder": 1,
                    "title": "generalMqttTopicNameTitle",
                    "description": "generalMqttTopicNameDescription",
                    "pattern": "^[^/+#]+/[^/+#]+$",
                    "_format": "wb-autocomplete",
                    "options": {
                      "patternmessage": "generalErrorRegexpPatternMessage",
                      "wb": {
                        "data": "devices"
                      }
                    },
                    "_minLength_note": "Device name (1) + slash (1) + topic name (1)",
                    "minLength": 3
                  }
                },
                "required": [
                  "mqttTopicName"
                ]
              }
            }
          }
        },
        "motionSensors": {
          "type": "object",
          "title": "lightControlMotionSensorsTitle",
          "propertyOrder": 7,
          "options": {
            "disable_collapse": true,
            "disable_edit_json": true,
            "disable_properties": true
          },
          "properties": {
            "delayToLightOff": {
              "type": "number",
              "title": "lightControlMotionSensorsDelayTitle",
              "default": 120,
              "propertyOrder": 1,
              "options": {
                "grid_columns": 12
              }
            },
            "sensorObjects": {
              "type": "array",
              "title": "generalSensorArrTitle",
              "_title_note": "When options-> compact: true - this title not shown",
              "description": "lightControlMotionSensorsArrDescription",
              "propertyOrder": 2,
              "_format": "table",
              "options": {
                "disable_collapse": true,
                "grid_columns": 12,
                "array_controls_top": true,
                "disable_array_delete_last_row": true,
                "disable_array_reorder": true,
                "wb": {
                  "disable_panel": true
                }
              },
              "items": {
                "type": "object",
                "title": "lightControlMotionSensorsObjTitle",
                "properties": {
                  "mqttTopicName": {
                    "type": "string",
                    "propertyOrder": 1,
                    "title": "generalMqttTopicNameTitle",
                    "description": "generalMqttTopicNameDescription",
                    "pattern": "^[^/+#]+/[^/+#]+$",
                    "_format": "wb-autocomplete",
                    "options": {
                      "patternmessage": "generalErrorRegexpPatternMessage",
                      "wb": {
                        "data": "devices"
                      }
                    },
                    "_minLength_note": "Device name (1) + slash (1) + topic name (1)",
                    "minLength": 3
                  },
                  "behaviorType": {
                    "type": "string",
                    "title": "generalBehaviorTypeTitle",
                    "enum": [
                      "whileValueHigherThanThreshold",
                      "whenEnabled"
                    ],
                    "options": {
                      "enum_titles": [
                        "lightControlMotionSensorsEnumWhileValue",
                        "When Enabled"
                      ]
                    },
                    "default": "whileValueHigherThanThreshold",
                    "propertyOrder": 2
                  },
                  "actionValue": {
                    "type": "number",
                    "title": "generalActionValueTitle",
                    "default": 170,
                    "propertyOrder": 3,
                    "options": {
                      "dependencies": {
                        "behaviorType": ["whileValueHigherThanThreshold"]
                      }
                    }
                  }
                },
                "required": [
                  "mqttTopicName",
                  "behaviorType"
                ]
              }
            }
          }
        },
        "openingSensors": {
          "type": "object",
          "title": "lightControlOpeningSensorsTitle",
          "propertyOrder": 8,
          "options": {
            "disable_collapse": true,
            "disable_edit_json": true,
            "disable_properties": true
          },
          "properties": {
            "delayToLightOff": {
              "type": "number",
              "title": "lightControlOpeningSensorsDelayTitle",
              "default": 180,
              "propertyOrder": 1,
              "options": {
                "grid_columns": 12
              }
            },
            "sensorObjects": {
              "type": "array",
              "title": "generalSensorArrTitle",
              "_title_note": "When options-> compact: true - this title not shown",
              "description": "lightControlOpeningSensorsArrDescription",
              "propertyOrder": 2,
              "_format": "table",
              "options": {
                "disable_collapse": true,
                "grid_columns": 12,
                "array_controls_top": true,
                "disable_array_delete_last_row": true,
                "disable_array_reorder": true,
                "wb": {
                  "disable_panel": true
                }
              },
              "items": {
                "title": "lightControlOpeningSensorsObjTitle",
                "type": "object",
                "properties": {
                  "mqttTopicName": {
                    "type": "string",
                    "propertyOrder": 1,
                    "title": "generalMqttTopicNameTitle",
                    "description": "generalMqttTopicNameDescription",
                    "pattern": "^[^/+#]+/[^/+#]+$",
                    "_format": "wb-autocomplete",
                    "options": {
                      "patternmessage": "generalErrorRegexpPatternMessage",
                      "wb": {
                        "data": "devices"
                      }
                    },
                    "_minLength_note": "Device name (1) + slash (1) + topic name (1)",
                    "minLength": 3
                  },
                  "behaviorType": {
                    "type": "string",
                    "propertyOrder": 2,
                    "title": "generalBehaviorTypeTitle",
                    "description": "lightControlOpeningSensorsBehaviorTypeDescription",
                    "enum": [
                      "whenDisabled",
                      "whenEnabled"
                    ],
                    "default": "whenEnabled",
                    "options": {
                      "enum_titles": [
                        "lightControlOpeningSensorsEnumWhenDisabled",
                        "lightControlOpeningSensorsEnumWhenEnabled"
                      ]
                    }
                  }
                },
                "required": [
                  "mqttTopicName",
                  "behaviorType"
                ]
              }
            }
          }
        }
      },
      "required": [
        "componentVersion",
        "scenarioType",
        "enable",
        "name",
        "id_prefix",
        "lightDevices",
        "motionSensors",
        "openingSensors",
        "lightSwitches"
      ]
    }
  },
  "properties": {
    "configVersion": {
      "type": "integer",
      "title": "Config version",
      "minimum": 1,
      "maximum": 256,
      "default": 1,
      "options": {
        "hidden": true
      }
    },
    "scenarios": {
      "type": "array",
      "title": "Scenarios",
      "items": {
        "$ref": "#/definitions/scenario"
      },
      "options": {
        "disable_collapse": true,
        "disable_array_reorder": true,
        "disable_array_delete_last_row": true,
        "enable_array_copy": true
      },
      "_format": "tabs"
    }
  },
  "required": [
    "configVersion",
    "scenarios"
  ],
